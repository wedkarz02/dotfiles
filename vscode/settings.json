{
    "workbench.iconTheme": "vscode-icons",
    "prettier.tabWidth": 4,
    "editor.detectIndentation": false,
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "GitBash": {
            "path": "G:\\ProgramFiles\\Git\\bin\\bash.exe"
        },
        "Ubuntu (WSL)": {
            "path": "C:\\Windows\\System32\\wsl.exe",
            "args": ["-d", "Ubuntu"]
        }
    },
    "terminal.integrated.defaultProfile.windows": "Ubuntu (WSL)",
    "editor.fontSize": 12,
    "terminal.integrated.fontSize": 14,
    "workbench.tree.indent": 12,
    "editor.fontFamily": "JetBrains Mono",
    "editor.fontLigatures": false,
    "editor.rulers": [80],
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorSurroundingLines": 8,
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[python]": {
        "editor.formatOnSave": true
    },
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.formatOnSave": true
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true,
        "editor.acceptSuggestionOnCommitCharacter": false
    },
    "[sql]": {
        "editor.formatOnSave": true
    },
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml",
        "editor.formatOnSave": true,
        "editor.autoIndent": "full"
    },
    "[dockercompose]": {
        "editor.defaultFormatter": "redhat.vscode-yaml",
        "editor.formatOnSave": true
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "keyword.other.dotenv",
                "settings": {
                    "foreground": "#FF000000"
                }
            }
        ]
    },
    "go.toolsManagement.autoUpdate": true,
    "hexeditor.columnWidth": 16,
    "hexeditor.showDecodedText": true,
    "hexeditor.defaultEndianness": "little",
    "hexeditor.inspectorType": "aside",
    "todohighlight.isEnable": true,
    "todohighlight.isCaseSensitive": true,
    "todohighlight.keywords": [
        {
            "text": "TODO:",
            "color": "#fff",
            "backgroundColor": "#109126"
        },
        {
            "text": "NOTE:",
            "color": "#000",
            "backgroundColor": "#f8fc00"
        },
        {
            "text": "SECTION:",
            "color": "#000",
            "backgroundColor": "#07f793"
        },
        {
            "text": "BUG:",
            "color": "#fff",
            "backgroundColor": "#ff0000"
        },
        {
            "text": "FIXME:",
            "color": "#fff",
            "backgroundColor": "#de3ea9"
        },
        {
            "text": "IDEA:",
            "color": "#000",
            "backgroundColor": "#00f4fc"
        },
        {
            "text": "WARNING:",
            "color": "#fff",
            "backgroundColor": "#e67700"
        }
    ],
    "editor.multiCursorLimit": 100,
    "markdown-preview-github-styles.colorTheme": "dark",
    "markdown-preview-github-styles.darkTheme": "dark_dimmed",
    "lldb.suppressUpdateNotifications": true,
    "vsicons.dontShowNewVersionMessage": true,
    "workbench.colorTheme": "Tokyo Night Storm",
    "workbench.colorCustomizations": {
        "editorLineNumber.foreground": "#7287a8",
        "editorLineNumber.activeForeground": "#d2e1fa"
    },
    "editor.lineNumbers": "relative",
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "workbench.startupEditor": "none",
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.cursorStylePerMode.insert": "block",
    "vim.leader": "<space>",
    "vim.normalModeKeyBindings": [
        {
            "before": ["leader", "w"],
            "commands": ["workbench.action.files.save"]
        },
        {
            "before": ["leader", "q"],
            "commands": [":q"]
        },
        {
            "before": ["leader", "s", "f"],
            "commands": ["workbench.action.quickOpen"]
        },
        {
            "before": ["leader", "i"],
            "after": ["g", "h"]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [">"],
            "commands": ["editor.action.indentLines"]
        },
        {
            "before": ["<"],
            "commands": ["editor.action.outdentLines"]
        },
        {
            "before": ["<S-j>"],
            "commands": ["editor.action.moveLinesDownAction"]
        },
        {
            "before": ["<S-k>"],
            "commands": ["editor.action.moveLinesUpAction"]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["<C-d>"],
            "after": ["<C-d>", "z", "z"]
        },
        {
            "before": ["<C-u>"],
            "after": ["<C-u>", "z", "z"]
        }
    ],
    "vim.insertModeKeyBindings": [
        {
            "before": [ "j", "k" ],
            "after": [ "<Esc>" ]
        }
    ],
    "editor.stickyScroll.enabled": false,
    "breadcrumbs.enabled": false
}
